[Unit]
Description=uWSGI instance
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=/var/www/website
Environment="PATH=/var/www/.virtualenvs/pharmadataassociates/bin"
ExecStart=/var/www/.virtualenvs/pharmadataassociates/bin/uwsgi --ini /var/www/website/config/uwsgi/website.ini

[Install]
WantedBy=multi-user.target
